(set-info :smt-lib-version v2.6)
(set-logic QF_NRA)
(declare-const x0 Real)
(declare-const x1 Real)
(declare-const x2 Real)
(assert (> x0 0))
(assert (> x1 0))
(assert (> x2 0))
(assert (= (- (+ (* x0 x0 x0) (* x1 x1 x1)) (* x1 x2)) 0))
(assert (let ((?a1 (+ x1 0)) (?a2 (+ x2 0))) (= ?a1 ?a2)))
(check-sat)
(exit)
